#Rapper {background-image: url("Images/Eureka 2.jpg");
	  background-repeat: no-repeat ;
	  background-size: cover ;
	  background-attachment: fixed;
	  width: 80% ;
	  margin-left: auto ; margin-right: auto ; 
	  display: grid ;
	  grid-template-columns: 5% 90% 5%;
	  grid-template-rows: 1% 89 10%;
	  text-align: center ;}
#REDACTED {background-color: #111111;
	  background-repeat: no-repeat ;
	  background-size: cover ;
	  background-attachment: fixed;
	  width: 80% ;
	  margin-left: auto ; margin-right: auto ; 
	  display: grid ;
	  grid-template-columns: 5% 90% 5%;
	  grid-template-rows: 1% 89 10%;}
header {grid-column: 1/4 ; grid-row: 1/2 ;}
nav {grid-column: 1/4 ; grid-row: 1/2 ;} 
main {grid-column: 2/3 ; grid-row: 2/3 ;}
footer {grid-column:1/4 ; grid-row: 3/4;
	    border-top: 3px solid black}

	  
	  
nav {font-family: "Times New Roman", Times, serif ;
	 font-size: 25px ; font-weight: bold ;
	 text-decoration: none ;
	 text-align: center ;
	 border-radius: 10px ;
	 box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19); 
	 word-spacing: 30px ;}
	 
nav a {color: black ;
	   text-shadow: 2px 2px 2px 2px white ;}
nav a:hover {color: white ;}
	 
